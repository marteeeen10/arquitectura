<div id="cartContainer">
  <h1>Carts</h1>
  <div id="productContainer">
    
 {{#each carts}}
   <div class="carritoSeparado">
    <p>Carrito : {{_id}}</p>
      {{#each products}}
       <div id="cardCart">
      <div> 
      <img src="{{product.thumbnail}}" alt="">
      <p>Producto :{{product.title}}</p>
      </div>
      <p>Categoria :{{product.category}}</p>
      <p>Cantidad :{{quantity}}</p>
      </div>
       {{/each}}

   </div>
      
  
  {{/each}}
  
  </div>
  
 

</div>